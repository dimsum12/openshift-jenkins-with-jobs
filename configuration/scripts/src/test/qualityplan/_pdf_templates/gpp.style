styles:      
    main-title:
        spaceAfter: 6
        spaceBefore: 12
        fontSize: 200%
        fontName: stdBold
        alignment: TA_RIGHT
        
    footer:
      parent: normal
      alignment: TA_RIGHT
    
    heading:
        backColor: #FFFFFF
        borderColor: #FFFFFF
        borderPadding: [0, 0, 0, 0]
        borderWidth: 0
        fontName: stdBold
        fontSize: 10
        leading: 10
        leftIndent: 0
        spaceBefore: 16
        textColor: #000000
    
    figure:
      spaceAfter: 1200
      spaceBefore: 1200
      parent: bodytext
      alignment: TA_CENTER
      commands: []
             [VALIGN, [ 0, 0 ], [ -1, -1 ], TOP ]
             [ALIGN, [ 0, 0 ], [ -1, -1 ], CENTER ]
      colWidths: [100%]
    
    figure-caption:
      spaceAfter: 1200
      spaceBefore: 1200
      parent: bodytext
      fontName: stdItalic
      alignment: TA_CENTER
      
    heading:
      parent: normal
      keepWithNext: true
      spaceBefore: 12
      spaceAfter: 6
      backColor: null
      borderColor: null
      
    table-heading:
      parent : heading
      backColor : #006699
      alignment : TA_CENTER
      valign : BOTTOM
      borderPadding : 0
      textColor: white
      
    table:
      spaceBefore:6
      spaceAfter:0
      alignment: TA_CENTER
      commands: []
             [VALIGN, [ 0, 0 ], [ -1, -1 ], TOP ]
             [INNERGRID, [ 0, 0 ], [ -1, -1 ], 0.25, #006699 ]
             [ROWBACKGROUNDS, [0, 0], [-1, -1], [#DBECF6, white]]
             [BOX, [ 0, 0 ], [ -1, -1 ], 0.25, #006699 ]

fontsAlias:
    stdFont: Helvetica
    stdBold: Helvetica-Bold
    stdItalic: Helvetica-Oblique
    stdBoldItalic: Helvetica-BoldOblique
    stdSans: Helvetica
    stdSansBold: Helvetica-Bold
    stdSansItalic: Helvetica-Oblique
    stdSansBoldItalic: Helvetica-BoldOblique
    stdMono: Courier
    stdMonoItalic: Courier-Oblique
    stdMonoBold: Courier-Bold
    stdMonoBoldItalic: Courier-BoldOblique
    stdSerif: Times-Roman